{
  "name": "vite-plugin-civet",
  "version": "0.1.0",
  "description": "Vite plugin for civet",
  "license": "MIT",
  "author": "Lorefnon <lorefnon@tutanota.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lorefnon/vite-plugin-civet.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "play": "pnpm run --filter ./playground dev",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "homepage": "https://github.com/lorefnon/vite-plugin-civet#readme",
  "bugs": {
    "url": "https://github.com/lorefnon/vite-plugin-civet/issues"
  },
  "dependencies": {
    "@danielx/civet": "^0.5.0",
    "@rollup/pluginutils": "^4.2.1",
    "cheerio": "1.0.0-rc.12",
    "vite": "^2.9.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@types/node": "^17.0.45",
    "bumpp": "^7.2.0",
    "eslint": "^8.19.0",
    "pnpm": "^7.5.0",
    "tsup": "^5.12.9",
    "typescript": "^4.7.4",
    "vitest": "^0.10.5"
  }
}
